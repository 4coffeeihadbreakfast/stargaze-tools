{
  "name": "@stargazezone/stars-cli",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "stars-cli": "./build/src/index.js"
  },
  "devDependencies": {
    "@cosmjs/cli": "^0.27.1",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^17.0.18"
  },
  "dependencies": {
    "@pinata/sdk": "^1.1.23",
    "@types/yargs": "^17.0.8",
    "asciify-image": "^0.1.8",
    "chalk": "^5.0.0",
    "inquirer": "^8.2.0",
    "ipfs-car": "^0.6.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "yargs": "^17.3.1"
  },
  "scripts": {
    "build": "tsc",
    "pack-images": "ipfs-car --pack images --output images.car",
    "pack-metadata": "ipfs-car --pack metadata --output metadata.car",
    "account": "cosmwasm-cli --init scripts/account.ts --code 'process.exit(0)'",
    "init": "cosmwasm-cli --init scripts/init.ts --code 'process.exit(0)'",
    "mint": "cosmwasm-cli --init scripts/mint.ts --code 'process.exit(0)'",
    "load": "cosmwasm-cli --init scripts/load.ts",
    "pinata-upload": "ts-node scripts/pinata-upload.ts"
  }
}
